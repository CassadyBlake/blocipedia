<%= wiki.users.each do |user| %>
  <div class="media">
    <div class="media-body">
      <strong><%= user.username %></strong> <%= link_to "Delete Collaborator", wiki_collaborator_path, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this collaborator?' } %>
    </div>
  </div>
<% end %>

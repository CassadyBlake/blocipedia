<h1>All Wikis</h1>

<div>
  <p><%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %></p>
</div>

<% @wikis.each do |wiki| %>
  <% if can_view_wiki?(wiki)  %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to wiki.title, wiki %>
      </h4>
    </div>
  </div>
  <% end %>
<% end %>
